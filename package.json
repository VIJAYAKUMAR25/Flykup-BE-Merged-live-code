{
  "name": "flykup-backend",
  "version": "1.0.0",
  "description": "flykup - India's first live auction platform",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node index.js",
    "dev": "nodemon index.js",
    "update-geoip": "mkdir -p geoip-data && curl --ssl-no-revoke -sS \"https://download.maxmind.com/app/geoip_download?edition_id=GeoLite2-City&license_key=${MAXMIND_LICENSE_KEY}&suffix=tar.gz\" -o geoip.tar.gz && tar -xzf geoip.tar.gz --strip-components=1 -C geoip-data && rm geoip.tar.gz",
    "postinstall": "npm run update-geoip"
},
  "keywords": [
    "node.js",
    "javacsript"
  ],
  "author": "Think strive",
  "license": "ISC",
  "dependencies": {
    "@api/streaming-apis": "file:.api/apis/streaming-apis",
    "@aws-sdk/client-s3": "^3.774.0",
    "@azure/storage-blob": "^12.27.0",
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "chalk": "^5.4.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "device-detector-js": "^3.0.3",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-async-handler": "^1.2.0",
    "firebase-admin": "^13.3.0",
    "geoip-lite": "^1.4.10",
    "jsonwebtoken": "^9.0.2",
    "maxmind": "^4.3.27",
    "mongoose": "^8.9.5",
    "multer": "^1.4.5-lts.2",
    "node-cron": "^4.1.1",
    "node-media-server": "^4.0.7",
    "nodemailer": "^6.9.16",
    "nodemon": "^3.1.9",
    "sharp": "^0.33.5",
    "socket.io": "^4.8.1",
    "ua-parser-js": "^2.0.4",
    "uuid": "^11.1.0",
    "validator": "^13.12.0"
  }
}
